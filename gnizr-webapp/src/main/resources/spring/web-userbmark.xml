<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
	"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="listForUserBookmark" class="com.gnizr.web.action.bookmark.ListForUserBookmark" singleton="false">
		<property name="forUserPager" ref="forUserPager"/>
		<property name="forUserManager" ref="forUserManager"/>
	</bean>
	
	<bean id="editForUser" class="com.gnizr.web.action.bookmark.EditForUser" singleton="false">
		<property name="forUserManager" ref="forUserManager"/>
	</bean>
	
	<bean id="listBookmark" class="com.gnizr.web.action.bookmark.ListUserBookmark" singleton="false">
		<property name="bookmarkPager" ref="bookmarkPager"/>
		<property name="userManager" ref="userManager"/>
		<property name="tagManager" ref="tagManager"/>
	</bean>
	
	<bean id="editBmark" class="com.gnizr.web.action.bookmark.EditBookmark" singleton="false">
		<property name="bookmarkManager" ref="bookmarkManager"/>
		<property name="linkManager" ref="linkManager"/>
	</bean>
	
	<bean id="importDeliciousPosts" class="com.gnizr.web.action.bookmark.ImportDeliciousPosts" singleton="false">
		<property name="userManager" ref="userManager"/>
		<property name="bookmarkManager" ref="bookmarkManager"/>
		<property name="folderManager" ref="folderManager"/>
	</bean>
	
	<bean id="manageBookmarks" class="com.gnizr.web.action.bookmark.ManageBookmarks" singleton="false">
		<property name="bookmarkManager" ref="bookmarkManager"/>
		<property name="folderManager" ref="folderManager"/>
	</bean>
	
	<bean id="listBookmarkHasGeomMarker" class="com.gnizr.web.action.bookmark.ListBookmarkHasGeomMarker" singleton="false">
		<property name="bookmarkManager" ref="bookmarkManager"/>
		<property name="folderManager" ref="folderManager"/>
        <property name="userManager" ref="userManager"/>
	</bean>
	
	<bean id="fetchBookmark" class="com.gnizr.web.action.bookmark.FetchBookmark" singleton="false">
		<property name="bookmarkManager" ref="bookmarkManager"/>
	</bean>
</beans>